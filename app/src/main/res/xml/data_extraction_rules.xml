<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="database" path="personal_finance_database" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow data transfer between devices -->
        <include domain="database" path="personal_finance_database" />
    </device-transfer>
</data-extraction-rules>
